<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("partials/header.ejs") %>
</head>

<body>
    <%- include("partials/top.ejs") %>
    <div class="container">
        <div class="row">
            <% data.forEach(function(producto){ %>
                <div class="card">
                    <div class="card-body">
                        <p>
                            <%= producto.nombre %>
                        </p>
                        <p> ID PRODUCTO: <%= producto.id %>
                        </p>
                        <img src="<%=producto.image%>.jpg" class="card-img-top">
                    </div>
                </div>
                <% }) %>
        </div>
    </div>
</body>

</html>